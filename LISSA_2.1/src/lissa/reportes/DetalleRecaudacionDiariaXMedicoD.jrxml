	<parameter name="FECHA_COMPROBANTE" class="java.util.Date"/>
	<parameter name="ID_MEDICO" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[Select c.fecha_boleta as FECHA_REG,
ser.denominacion, count(ser.denominacion), ser.costo, md.id_medico
from comprobante as c inner join items_comprobante as ic
on c.id_comprobante = ic.id_boleta inner join servicio ser
on ic.id_servicio = ser.id_servicio inner join medico md
on ic.id_medico_deriva = md.id_medico
where c.fecha_boleta = $P{FECHA_COMPROBANTE} AND ic.id_medico_deriva = $P{ID_MEDICO}
GROUP BY c.fecha_boleta, ser.denominacion, ser.costo, md.id_medico
order by c.fecha_boleta desc]]>
	</queryString>
	<field name="fecha_reg" class="java.sql.Date"/>
	<field name="denominacion" class="java.lang.String"/>
	<field name="count" class="java.lang.Long"/>
	<field name="costo" class="java.math.BigDecimal"/>
	<field name="id_medico" class="java.lang.Integer"/>
	<variable name="SUM_MEDICO" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{count}*$F{costo}]]></variableExpression>
	</variable>
