	<queryString>
		<![CDATA[select
f.nombre,
e.cantidad,
e.lote,
e.fecha_vencimiento,
e.precio_compra,
e.precio_compra_unitario,
e.porc_desc1,
e.porc_desc2,
e.descuento_item,
a.valor_neto,
a.valor_igv,
a.valor_total,
a.valor_bruto,
a.valor_descuento
from detalle_compra e inner join compra a
on e.id_compra = a.idcompra inner join producto f
on e.id_producto = f.idproducto
where a.idcompra = $P{ID_COMPRA}
order by e.iddetallecompra]]>
	</queryString>
